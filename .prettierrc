{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "semi": true,
  "printWidth": 80,
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "overrides": [
    {
      "files": [
        "**/*.css",
        "**/*.html",
        "**/*.md"
      ],
      "options": {
        "singleQuote": false
      }
    }
  ]
}